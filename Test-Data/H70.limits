



; Template file for limits for H/0
;
; Joerg Werner 15.02.2006
;
; Adaptions for H70:  
; Martin Butkus 06.12.2007

MARGIN=0.1

VREF=0.8*VREFTRIM		; default 0.8V
VREFERR=MARGIN*VREF
VREFGLOBAL=0.95/0.8*VREF                ; default 0.95V
VREFGLOBALERR=MARGIN*VREFGLOBAL

; from here on everything depends on VREFGLOBAL ...

VINT=MIN(VREFGLOBAL/0.95*1.2*VINTTRIM,VDD-0.025)		; default 1.2V
VINTERR=MARGIN*VINT
VINT_VMONW=VINT
VINT_VMONWERR=VINTERR

VNWLL=VREFGLOBAL*-0.6/0.95*VNWLLTRIM		; default -0.6V
VNWLLERR=MAX(MARGIN*(-VNWLL),0.1)
VNWLL_VMONW=2*(VINT-VNWLL)/3+VNWLL
VNWLL_VMONWERR=MARGIN*VNWLL_VMONW


;VBLH=MIN(VREFGLOBAL/0.95*1.2*VBLHTRIM,VDD-0.025)		; default 1.2V
VBLH=MIN(VREFGLOBAL/0.95*1.2*VBLHTRIM,VDD-0.025)		
VBLHERR=MARGIN*VBLH
VBLH_VMONW=VBLH
VBLH_VMONWERR=VBLHERR

VBLEQ=VBLH/1.2*0.62*VBLEQTRIM		; default 0.62V
VBLEQERR=MAX(MARGIN*VBLEQ,0.1)
VBLEQ_VMONW=VBLEQ
VBLEQ_VMONWERR=VBLEQERR

VPL=VBLH/1.2*0.6*VPLTRIM			; default 0.6V
VPLERR=MAX(MARGIN*VPL,0.1)
VPL_VMONW=VPL
VPL_VMONWERR=VPLERR

VBB=VREFGLOBAL/0.95*VBBTRIM		; default 0V
VBBERR=MAX(MARGIN*(VBB),0.1)
VBB_VMONW=3*(VINT-VBB)/5+VBB
VBB_VMONWERR=MARGIN*VBB_VMONW

VPP=VREFGLOBAL/0.95*2.65*VPPTRIM		; default 2.65V
VPPERR=MARGIN*VPP
VPP_VMONW=VPP/3
VPP_VMONWERR=MARGIN*VPP_VMONW

; VDLL=VINT*VDLLTRIM
; VDLLERR=0.1*VDLL
; VDLL_VMONW=VDLL
; VDLL_VMONWERR=VDLLERR

; VBURN=-17+4/0.22*VREF
; VBURNERR=1.0

VEQL=VREFGLOBAL/0.95*1.6*VEQLTRIM		; default 1.6V
VEQLERR=MAX(MARGIN*VEQL,0.1)
VEQL_VMONW=VEQL/2 - 0.03
VEQL_VMONWERR=MARGIN*VEQL_VMONW

IREF=-0.000020*IREFTRIM                                                               ; XXX ???
IREFERR=MARGIN*IREF

GND_VMONW=0
GND_VMONWERR=0.1
