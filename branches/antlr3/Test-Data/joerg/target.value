; Target when VREF is forced
VREF_TARGET=0.8V

; Chip-Individual voltage trimming
VIND_TARGET=0.950V
VIND_STEPS=32
VIND_CHANNEL=VMONW_CHANNEL
VIND_PAT="h1vrefp"

; Chip-individual current trimming
IIND_TARGET=20uA
IIND_VOLTAGE=0.5V
IIND_STEPS=16
IIND_CHANNEL=VMONW_CHANNEL
IIND_PAT="h1irefp"

; What trimming is active (0=both,1=I only,2=V only)
WHATTRIM=2
